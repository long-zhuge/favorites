{
  "entry": "src/index.js",
  "extraBabelPlugins": [
    "transform-runtime",
    "transform-decorators-legacy",
    "transform-class-properties",
    ["import", { "libraryName": "antd", "libraryDirectory": "es", "style": true }]
  ],
  "env": {
    "development": {
      "extraBabelPlugins": [
        "dva-hmr"
      ]
    }
  },
  "externals": {},
  "ignoreMomentLocale": true,
  "theme": "./src/theme.js",
  "hash": true,
  "publicPath": "./",
  "html": { "template": "./src/index.ejs" }
}
